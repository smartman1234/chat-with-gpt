{
  "name": "@dqbd/tiktoken",
  "version": "1.0.0-alpha.1",
  "description": "Javascript bindings for tiktoken",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dqbd/tiktoken"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  },
  "files": [
    "**/*"
  ],
  "main": "tiktoken.node.js",
  "types": "tiktoken.d.ts",
  "exports": {
    ".": {
      "types": "./tiktoken.d.ts",
      "node": "./tiktoken.node.js",
      "default": "./tiktoken.js"
    },
    "./bundler": {
      "types": "./bundler.d.ts",
      "default": "./bundler.js"
    },
    "./init": {
      "types": "./init.d.ts",
      "default": "./init.js"
    },
    "./tiktoken_bg.wasm": {
      "types": "./tiktoken_bg.wasm.d.ts",
      "default": "./tiktoken_bg.wasm"
    }
  }
}